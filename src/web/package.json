{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "build:docker": "vue-cli-service build --dest ../app/dist/web",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.24.0",
    "core-js": "3.24.1",
    "vue": "2.6.11",
    "vue-router": "3.5.3",
    "vuetify": "^2.6.14",
    "vuex": "3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.8.0",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": [
        "error",
        {
          "ignores": [
            "Form",
            "Profile",
            "Login",
            "Home",
            "Grid"
          ]
        }
      ],
      "vue/max-len": [
        "error",
        {
          "code": 120,
          "ignoreStrings": true,
          "ignoreComments": true,
          "ignoreUrls": true
        }
      ],
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
